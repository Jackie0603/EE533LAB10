<?xml version="1.0" encoding="UTF-8"?>
<nf:module xmlns:nf="http://www.NetFPGA.org/NF2_register_system" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.NetFPGA.org/NF2_register_system NF2_register_system.xsd ">
	<nf:name>processor</nf:name>
	<nf:prefix>processor</nf:prefix>
	<nf:location>udp</nf:location>
	<nf:description>Registers for processor</nf:description>
	<nf:blocksize>64</nf:blocksize>
	<nf:registers>
		<nf:register>
		  <nf:name>check_addr</nf:name>
			<nf:description>Lab 6 address</nf:description>
			<nf:type>generic_software32</nf:type>
		</nf:register>
        <nf:register>
		  <nf:name>check</nf:name>
			<nf:description>Check flag for dmem</nf:description>
			<nf:type>generic_software32</nf:type>
		</nf:register>

        <nf:register>
		  <nf:name>check_high</nf:name>
			<nf:description>Output Data High</nf:description>
			<nf:type>generic_hardware32</nf:type>
		</nf:register>

        <nf:register>
		  <nf:name>check_low</nf:name>
			<nf:description>Output Data Low</nf:description>
			<nf:type>generic_hardware32</nf:type>
		</nf:register>
	</nf:registers>
</nf:module>

